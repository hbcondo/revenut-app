{
  "expo": {
    "name": "Revenut",
    "scheme": "revenut",
    "slug": "revenut-app",
    "description": "Saas Metrics in a Nutshell",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "primaryColor": "#E78E3A",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8EFED"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F8EFED"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "scope": "/"
    }
  }
}
